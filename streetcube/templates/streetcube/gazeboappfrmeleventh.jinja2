{% include "include/page-header.jinja2" %}

<section class="bg-dark2">
    <h5 class="text-center pt-3 text-green"><b>GAZEBO APPLICATION FORM</b></h5>
    <div class="text-center">
		<img width="100" src="{{url_for('static', filename='img/icons/cube.svg')}}">
	</div>
	<div class="container">
	    <div class="row">
	    <div class="col-md-3"></div>
	    <div class="col-md-6">
	       

        <div class="form-box mt-2">
            
                 <h6><b>Part 8: Business</b></h6>
              
            
            <form action="" method="POST" name="gazeboappfrmeleventh">
                <p><i>Business Location</i></p>
               
                <div class="form-group">
                    <label for="colFormLabelSm" class="col-form-label col-form-label-sm text-left">Do you have a specific location in mind for the placement of a StreetCube?</label>
                    <input type="text" class="form-control form-control-sm" id="" name="specific_location" value="{{ga_prt_11th_up.specific_location}}">
                </div>
                
                
                <div class="form-group">
                    <label for="colFormLabelSm" class="col-form-label col-form-label-sm text-left">Have you approached the land owner / authority for this piece of land?</label>
                    <input type="text" class="form-control form-control-sm" id="" name="land_owner" value="{{ga_prt_11th_up.land_owner}}">
                </div>
                
                <div class="form-group">
                    <label for="colFormLabelSm" class="col-form-label col-form-label-sm text-left">Please state who has jurisdiction / authority for this piece of land:</label>
                    <input type="text" class="form-control form-control-sm" id="" name="jurisdict_authority" value="{{ga_prt_11th_up.jurisdict_authority}}">
                </div>
                
                <div>
                <label for="colFormLabelSm" class="col-form-label col-form-label-sm">Storage required?</label>
                </div>
                <div class="form-check form-check-inline error-msg-set">
                  <input class="form-check-input" type="radio" name="is_storage_req" id="" value="1" {% if ga_prt_11th_up.is_storage_req%} checked{%endif%}>
                  <label class="form-check-label" for="inlineRadio6">YES</label>
                </div>
                <div class="form-check form-check-inline error-msg-set">
                  <input class="form-check-input" type="radio" name="is_storage_req" id="" value="0" {% if not ga_prt_11th_up.is_storage_req%} checked{%endif%}>
                  <label class="form-check-label" for="inlineRadio7">NO</label>
                </div>
                <br>
                <br>
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2482.5908642711165!2d-0.12425368482069059!3d51.52072167963735!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x48761b36a3027021%3A0x35e5db41d76376a8!2sStreetCube!5e0!3m2!1sen!2sin!4v1612785465722!5m2!1sen!2sin" width="100%" height="200" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                
                
                
                
                
                <div class="text-right">
                    <button type="submit" class="btn-none"><img height="35" draggable="false" src="{{url_for('static', filename='img/icons/con-btn.svg')}}"></button>
                </div>
                
            </form>
        </div>
        <br>
                <br>
	    </div>
	    <div class="col-md-3"></div>
	</div>
	</div>
</section>


<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"> </script>
<script>

// Wait for the DOM to be ready
$(function() {
  $("form[name='gazeboappfrmeleventh']").validate({
    rules: {
      
      specific_location: "required",
      land_owner: "required",
      jurisdict_authority: "required",
      is_storage_req: "required",
     
    
     
    },
   
    messages: {
      specific_location: "Please enter your specific location",
      land_owner: "Please enter land owner",
      jurisdict_authority: "Please enter jurisdict authority",
      is_storage_req: "Please select any of them",
     
     
     
      
    },
    submitHandler: function(form) {
      form.submit();
    }
  });
});
</script>

{% include "include/page-footer.jinja2" %}