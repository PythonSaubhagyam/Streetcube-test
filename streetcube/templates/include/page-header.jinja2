		<style>
            .box_menu {
                padding: 1rem 0rem 1rem 0rem;
                text-align:unset;
            }
            .bg-section{
                background:rgb(8 8 8 / 60%);
                height:100%;
            }
            .box_menu h5{
                position:unset;
            }
            .bg-green{
                height:50px;
                background: #0D8642;
            }
            .text-green{
                color: #0D8642;
            }
            body{
                 background:#1B1B1B;
            }
            .bg-dark2{
                background:#1B1B1B;
                /*max-height:calc(100vh - 50px);*/
                color: #fff;
            }
            .form-box{
                padding:15px;
                border:2px solid #fff;
            }
            .form-control{
                background:none;
                border:2px solid #fff;
            }
            .form-control-sm {
                height: calc(1.8125rem + 2px) !important;
                padding: .25rem .5rem !important;
                font-size: .875rem !important;
                line-height: 1.5 !important;
                border-radius: 0 !important;
                background: unset;
                border: 2px solid #fff;
                color: #fff;
            }
            .form-control-sm:focus{
                /*color:#fff;*/
            }
            .form-group {
                margin-bottom: 7px;
            }
            .btn-none{
                background: none;
                border:none;
                cursor:pointer;
            }
            .btn-none:focus{
                outline:none;
            }
            .gj-icon{
                top: 6px !important;
            }
            .col-form-label-sm{
                text-align:right;
            }
            
            /*Mobile devices*/
             @media only screen and (max-width : 768px) {
             	.col-form-label-sm{
                    text-align:left;
                }
            }	
            /* Medium Devices, Desktops */
             @media only screen and (max-width : 992px) {
                 .col-form-label-sm{
                    text-align:left;
                }
            }
            
