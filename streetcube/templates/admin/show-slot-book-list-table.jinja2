{% extends 'admin/layout.jinja2' %}
{% block body %}
<div class="clearfix"></div>
  <div class="content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-header"><i class="fa fa-table"></i> Data Exporting</div>
            <div class="card-body">
              <div class="table-responsive">
              <table id="example" class="table table-bordered">
                <thead>
                    <tr>
                        <th>Member Name</th>
                        <th>Book By</th>
                        <th>Choose Slot</th>
                        <th>Select Date</th>
                        <th>Booking Cost</th>
                        <th>Register Date</th>
                        <th>Payment Status</th>
                    </tr>
                </thead>
                <tbody>
                {% for slist in slot_d_list %}
                    <tr>
                        <td>{{ slist.uname }}</td>
                        <td>{{ slist.book_by }}</td>
                        <td>{{ slist.choose_slot }}</td>
                        <td>{{ slist.select_date }}</td>
                        <td>{{ slist.booking_cost }}</td>
                        <td>{{ slist.register_date}}</td>
                        <td>{{slist.paid}}</td>
                      
                    </tr>
                {% endfor %}
                </tbody>
            </table>
            </div>
            </div>
          </div>
        </div>
      </div><!-- End Row-->
<!--start overlay-->
		  <div class="overlay toggle-menu"></div>
		<!--end overlay-->
    </div>
    <!-- End container-fluid-->

    </div><!--End content-wrapper-->
   <!--Start Back To Top Button-->
    <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
    <!--End Back To Top Button-->


{% endblock %}



