{% extends 'admin/layout.jinja2' %}
{% block body %}
<div class="clearfix"></div>
  <div class="content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-header"><i class="fa fa-table"></i> Data Exporting</div>
            <div class="card-body">
              <div class="table-responsive">
              <table id="example" class="table table-bordered">
                <thead>
                    <tr>
                        <th>Privileges</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Mobile</th>
                        <th>Register Date</th>
                        <th>User Came Url</th>
                        
                    </tr>
                </thead>
                <tbody>
                {% for list in user_list %}
                    <tr>
                        <td><a href="{{url_for('admin.user_block',id=list.id)}}"> Assign Privileges</a></td>
                        <td>{{ list.uname }}</td>
                        <td>{{ list.email }}</td>
                        <td>{{ list.mobile }}</td>
                        <td>{{ list.register_date }}</td>
                        <td>{%if list.cano_url%}{{list.cano_url}}
                        {%else%}
                        NA
                        {%endif%}
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
            </div>
            </div>
          </div>
        </div>
      </div><!-- End Row-->
<!--start overlay-->
		  <div class="overlay toggle-menu"></div>
		<!--end overlay-->
    </div>
    <!-- End container-fluid-->

    </div><!--End content-wrapper-->
   <!--Start Back To Top Button-->
    <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
    <!--End Back To Top Button-->


{% endblock %}



